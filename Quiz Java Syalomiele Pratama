import java.util.Scanner;
public class QuizJava {
    public static void main (String [] args){
        System.out.println("Masukkan Variabel 1 :");
        Scanner input = new Scanner(System.in);
        int a = input.nextInt();
        System.out.println("Masukkan Variabel 2");
        int b = input.nextInt();
        int jumlah = a+b ;
        System.out.println("Jumlah Variabel 1 + Variabel 2 =  "+jumlah);
        System.out.println("Dikerjakan oleh Syalomiele Pratama Agustinus Susanto (22201212)");
        
    }
}
